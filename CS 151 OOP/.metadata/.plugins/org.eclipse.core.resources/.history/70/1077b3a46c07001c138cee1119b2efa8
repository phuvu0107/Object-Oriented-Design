		Employee timothy = new Employee("Joe", "Smith", "834 Tersi Court", "Male", "104-849-1930",44, 189.9f, 200001, "contractor", 60);
		Employee george = new Employee("Joe", "Smith", "834 Tersi Court", "Male", "104-849-1930",44, 189.9f, 200001, "contractor", 60);
		Employee amy = new Employee("Joe", "Smith", "834 Tersi Court", "Male", "104-849-1930",44, 189.9f, 200001, "contractor", 60);
		
