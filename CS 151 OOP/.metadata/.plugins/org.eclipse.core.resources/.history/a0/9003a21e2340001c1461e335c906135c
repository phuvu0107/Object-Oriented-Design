
public class Hexagon extends Shape{
	//var
	private double side;
	
	//constructor
	Hexagon(){
		
	}
	
	Hexagon(double side){
		this.side = side;
	}
	
	//getter and setter
	public double getSide() {
		return this.side;
	}
	
	public void setSide(double side) {
		this.side = side;
	}
	
	//method
	@Override
	public void computeArea() {
		super.area = (3*Math.sqrt(3.0)*side)/2;
	}
}
